<h1>This is index page, Hello</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Detail</th>
  </tr>

  <% @tasks.each do |task| %>
    <tr>
      <td><%= task.name %></td>
      <td><%= task.detail %></td>
      <td><%= link_to 'Edit', edit_task_path(task) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New task', '/tasks/new' %>
