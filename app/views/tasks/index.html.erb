<%= Time.zone.now %>
<%= notice %>
<h1>This is index page, Hello</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Detail</th>
  </tr>

  <% @tasks.each do |task| %>
    <tr>
      <td><%= task.name %></td>
      <td><%= task.detail %></td>
      <td><%= link_to 'Edit', edit_task_path(task) %></td>
      <td><%= link_to 'Show', task_path(task) %></td>
      <td><%= link_to 'DELETE', task_path(task), method: :delete %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New task', '/tasks/new' %>
