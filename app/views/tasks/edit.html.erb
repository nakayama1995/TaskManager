<h1>This is edit page</h1>

<%= form_for @task , url: task_path(@task) do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :detail %><br>
    <%= f.text_area :detail %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', tasks_path %>
